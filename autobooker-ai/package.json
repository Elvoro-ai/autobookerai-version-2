{
  "name": "autobooker-ai",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.17 <=22.x",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "test": "vitest",
    "test:ci": "vitest run",
    "e2e": "playwright test",
    "e2e:ci": "playwright test --reporter=line",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "postinstall": "prisma generate || echo 'skip prisma generate'",
    "clean": "rm -rf .next",
    "build:analyze": "ANALYZE=true npm run build",
    "netlify:build": "npm ci && npm run build"
  },
  "dependencies": {
    "@auth/core": "0.34.2",
    "@auth/prisma-adapter": "^2.7.4",
    "@prisma/client": "^6.18.0",
    "autoprefixer": "^10.4.20",
    "framer-motion": "^11.16.0",
    "next": "^15.1.3",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.9.16",
    "postcss": "^8.4.49",
    "prisma": "^6.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/nodemailer": "^6.4.19",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.1.3",
    "playwright": "^1.49.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wait-on": "^8.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}